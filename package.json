{
  "name": "monoreact",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "next:dev": "pnpm -F @repo/next-app run dev",
    "next:build": "pnpm -F @repo/next-app run build",
    "next:start": "pnpm -F @repo/next-app run start",
    "vite:dev": "pnpm -F @repo/vite-spa run dev",
    "vite:build": "pnpm -F @repo/vite-spa run build",
    "vite:preview": "pnpm -F @repo/vite-spa run preview",
    "hono:dev": "pnpm -F @repo/hono-api run dev",
    "hono:build": "pnpm -F @repo/hono-api run build",
    "hono:start": "pnpm -F @repo/hono-api run start"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "0.0.5",
    "lefthook": "^2.0.4",
    "prettier": "3.6.2",
    "prettier-plugin-curly": "0.4.0"
  },
  "packageManager": "pnpm@10.23.0"
}
